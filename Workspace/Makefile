.PHONY: clean All

All:
	@echo "==========Building project:[ 15_Inheritance_Challenge - Debug ]=========="
	@cd "15_Inheritance_Challenge" && "$(MAKE)" -f  "15_Inheritance_Challenge.mk"
clean:
	@echo "==========Cleaning project:[ 15_Inheritance_Challenge - Debug ]----------"
	@cd "15_Inheritance_Challenge" && "$(MAKE)" -f  "15_Inheritance_Challenge.mk" clean
